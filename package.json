{
    "name": "shop-service",
    "version": "0.0.1",
    "description": "",
    "author": "icc",
    "private": true,
    "license": "UNLICENSED",
    "type": "commonjs",
    "scripts": {
        "prebuild": "rimraf dist",
        "migration:fake": "node -r tsconfig-paths/register node_modules/typeorm/cli-ts-node-commonjs.js migration:run -d src/ds.ts --fake",
        "migration:generate": "node -r tsconfig-paths/register node_modules/typeorm/cli-ts-node-commonjs.js migration:generate -d src/ds.ts",
        "migration:run": "node -r tsconfig-paths/register node_modules/typeorm/cli-ts-node-commonjs.js migration:run -d src/ds.ts",
        "migration:revert": "node -r tsconfig-paths/register node_modules/typeorm/cli-ts-node-commonjs.js migration:revert -d src/ds.ts",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && tsc --noEmit",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
        "docs": "swagger-jsdoc -d swagger.config.js -o shop-service.json src/modules/**/**.swagger.yaml src/modules/slug.swagger.yaml"
    },
    "dependencies": {
        "@nestjs/common": "^9.1.2",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.1.2",
        "@nestjs/mapped-types": "^1.1.0",
        "@nestjs/platform-express": "^9.1.2",
        "@nestjs/typeorm": "^9.0.0",
        "@types/ioredis": "^4.28.10",
        "be-core": "^1.3.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "date-fns": "^2.29.3",
        "ioredis": "^5.2.3",
        "lodash": "^4.17.21",
        "mysql2": "^2.3.3",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.7",
        "typeorm": "^0.3.10"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.1.3",
        "@nestjs/schematics": "^9.0.3",
        "@nestjs/testing": "^9.1.2",
        "@types/express": "^4.17.14",
        "@types/jest": "29.0.3",
        "@types/lodash": "^4.14.185",
        "@types/node": "^18.7.21",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^3.5.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.0",
        "jest": "29.0.3",
        "nodemon": "^2.0.20",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "swagger-jsdoc": "^6.2.1",
        "ts-jest": "29.0.2",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.1.0",
        "typescript": "^4.8.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/$1",
            "^config/(.*)$": "<rootDir>/../config/$1",
            "^@modules/(.*)$": "<rootDir>/modules/$1"
        },
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
